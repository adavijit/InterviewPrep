<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.avijit.lco">

    <uses-permission android:name="android.permission.INTERNET" />
    <uses-permission android:name="android.permission.ACCESS_NETWORK_STATE" />

    <application
        android:allowBackup="true"
        android:icon="@drawable/lco"
        android:label="InterviewPrep"
        android:roundIcon="@drawable/lco"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".splash_screen">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".answer" />
        <activity android:name=".MainActivity" />
        <activity android:name=".play" />
        <activity android:name=".homeScreen" />
        <activity android:name=".lco_ad" />
        <activity android:name=".about_us"></activity>
    </application>

</manifest>